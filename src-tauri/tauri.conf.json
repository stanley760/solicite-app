{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "solicite-app",
  "version": "0.1.0",
  "identifier": "io.adana.solicite",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [{
      "width": 640,
      "height": 480,
      "resizable": false,
      "fullscreen": false,
      "title": "数据统计程序",
      "transparent": true
    }],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
